<div>
  <h1></h1>
</div>
<div>
  <h1>投稿詳細</h1>
  <p>画像：<%= image_tag @building.get_image %></p>
  <p>建物名：<%= @building.shop_name %></p>
  <p>場所：<%= @building.place %></p>
  <p>説明：<%= @building.body %></p>
  <p>投稿者：<%= @building.user.name %></p>
  <p>投稿日：<%= @building.created_at.strftime('%Y/%m/%d') %></p>
  <% if @building.user == current_user %>
    <%= link_to "編集" %>
    <%= link_to "消去", building_path(@building.id), method: :delete %>
  <% end %>
</div>