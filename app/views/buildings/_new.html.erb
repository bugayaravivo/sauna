<h1>投稿作成</h1>
<%= form_with midel: building, url: buildings_path, method: :post do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>ショップ名</h4>
  <%= f.text_field :shop_name %>
  <h4>場所</h4>
  <%= f.text_field :place %>
  <h4>説明</h4>
  <%= f.text_area :body %>
  <%= f.submit "作成" %>
<% end %>